#include <stdio.h>
#include <stdlib.h>

void readArray(int arr[], int n)
{
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }
}

int findMax(int a, int b)
{
    if (a > b)
        return a;
    else
        return b;
}

int seriesMax(int arr[], int n)
{
    if (n == 1)
        return arr[0];

    return findMax(arr[n-1], seriesMax(arr, n-1));

}


int main()
{
    int n;
    scanf("%d", &n);

    int arr[1000];
    readArray(arr, n);

    int res = seriesMax(arr, n);
    printf("%d", res);

    return 0;
}
