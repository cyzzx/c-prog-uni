#include <stdio.h>
#include <stdlib.h>

int main()
{
    FILE *file1 = NULL;

    file1 = fopen("input.txt", "w");

    if (file1 == NULL)
    {
        puts("Cannot open file!");
        exit(1);
    }

    int n, j;
    scanf("%d", &n);

    for(int i = 0; i < n; i++)
    {
        scanf("%d", &j);
        fprintf(file1, "%d ", j);
    }

    fclose(file1);

    printf("Text written to file successfully.");
    return 0;

}
