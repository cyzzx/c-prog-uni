#include <stdio.h>
#include <stdlib.h>

int printArray(int n, int arr[])
{
    if (n == 0)
        printf("%d ", arr[0]);

    else
    {
        printArray(n-1, arr);
        printf("%d ", arr[n]);

    }
}

void readArray(int n, int arr[])
{
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }
}


int main()
{
    int n;
    int arr[100];
    scanf("%d", &n);

    readArray(n, arr);

    printArray(n-1, arr);
}
