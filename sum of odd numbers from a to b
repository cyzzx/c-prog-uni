#include <stdio.h>
#include <stdlib.h>

int findOddSum(int a, int b)
{
    int i = a+1;

    if (b == a)
    return 0;


    if (i % 2 != 0)
    {
        return i + findOddSum(i,b);
    }

    else
        findOddSum(i, b);

}
int main()
{
    int a, b;
    scanf("%d%d", &a, &b);


    int res = findOddSum(a, b);
    printf("%d", res);

    return 0;
}
