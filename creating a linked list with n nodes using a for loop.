#include <stdio.h>
#include <stdlib.h>

struct Node
{
    int ID;

    struct Node *next;
};

void displayList(struct Node* head)
 {
    struct Node* current = head;
    while (current != NULL) 
    {
        printf("%d \n", current->ID);
        current = current->next;
    }
    printf("NULL\n");
}

int main()
{
    int n;
    scanf("%d", &n);
    struct Node *head = NULL;
    struct Node *temp = NULL;

    for(int i = 1; i <= n; i++)
    {
        struct Node *newNode = NULL;
        newNode = (struct Node*) malloc(sizeof(struct Node));
        printf("Enter ID for user %d: \n", i);
        scanf("%d", &newNode->ID);
        newNode->next = NULL;

        if(head == NULL)
        {
            head = newNode;
        }

        else
        {
            temp->next = newNode;
        }

        temp = newNode;
    }

        displayList(head);
    return 0; 
}
